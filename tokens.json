{
  "global": {
    "global": {
      "colors": {
        "red": {
          "300": {
            "value": "#FCA5A5",
            "type": "color"
          },
          "500": {
            "value": "#EF4444",
            "type": "color"
          },
          "700": {
            "value": "#B91C1C",
            "type": "color"
          }
        },
        "black": {
          "value": "#000000",
          "type": "color"
        },
        "white": {
          "value": "#ffffff",
          "type": "color"
        },
        "primary": {
          "400": {
            "value": "#3138e4",
            "type": "color"
          },
          "800": {
            "value": "#11157e",
            "type": "color"
          },
          "900": {
            "value": "#030541",
            "type": "color"
          }
        },
        "yellow": {
          "value": "#E5AC00",
          "type": "color"
        }
      },
      "spacing": {
        "sm": {
          "value": "4",
          "type": "spacing"
        },
        "md": {
          "value": "{global.spacing.sm} * 2",
          "type": "spacing"
        },
        "lg": {
          "value": "{global.spacing.md} * 2",
          "type": "spacing"
        },
        "xl": {
          "value": "{global.spacing.lg} * 2",
          "type": "spacing"
        },
        "wrap": {
          "value": "$BaseValue * $default",
          "type": "spacing"
        }
      },
      "dark": {
        "5": {
          "value": "rgba({global.colors.black}, 5%)",
          "type": "color"
        },
        "10": {
          "value": "rgba({global.colors.black}, 10%)",
          "type": "color"
        },
        "20": {
          "value": "rgba({global.colors.black}, 20%)",
          "type": "color"
        },
        "25": {
          "value": "rgba({global.colors.black}, 25%)",
          "type": "color"
        }
      }
    },
    "typography": {
      "Body": {
        "value": {
          "fontFamily": "{font.Default}",
          "fontWeight": "$fontWeights.bodyRegular",
          "lineHeight": "$lineHeights.body",
          "fontSize": "$fontSizes.body",
          "letterSpacing": "0",
          "paragraphSpacing": "$paragraphSpacing.default",
          "textDecoration": "none",
          "textCase": "0"
        },
        "type": "typography"
      }
    },
    "bodyRegular": {
      "value": "Regular",
      "type": "fontWeights"
    },
    "bodyBold": {
      "value": "Bold",
      "type": "fontWeights"
    },
    "lineHeights": {
      "body": {
        "value": "140%",
        "type": "lineHeights"
      },
      "heading": {
        "value": "110%",
        "type": "lineHeights",
        "description": "Line Height Heading"
      }
    },
    "fontSizes": {
      "h1": {
        "value": "{fontSizes.h2} * {CalcValue}",
        "type": "fontSizes",
        "description": "Heading H1"
      },
      "h2": {
        "value": "{fontSizes.h3} * $CalcValue",
        "type": "fontSizes",
        "description": "Heading H2"
      },
      "h3": {
        "value": "{fontSizes.h4} * {CalcValue}",
        "type": "fontSizes",
        "description": "Heading H3"
      },
      "h4": {
        "value": "{fontSizes.h5} * {CalcValue}",
        "type": "fontSizes",
        "description": "Heading H4"
      },
      "h5": {
        "value": "{fontSizes.h6} * {CalcValue}",
        "type": "fontSizes",
        "description": "Heading H5"
      },
      "h6": {
        "value": "{fontSizes.body} * 1",
        "type": "fontSizes",
        "description": "Heading H6"
      },
      "body": {
        "value": "{BaseValue}",
        "type": "fontSizes"
      },
      "sm": {
        "value": "{fontSizes.body} * 0.85",
        "type": "fontSizes",
        "description": "Font Small"
      },
      "xs": {
        "value": "{fontSizes.body} * 0.65",
        "type": "fontSizes",
        "description": "Font Xsmall"
      }
    },
    "BaseValue": {
      "value": "14",
      "type": "fontSizes"
    },
    "CalcValue": {
      "value": "1.25",
      "type": "fontSizes"
    },
    "default": {
      "value": "8",
      "type": "sizing"
    },
    "xs": {
      "value": "$default / 2",
      "type": "sizing"
    },
    "s": {
      "value": "$default",
      "type": "sizing",
      "description": "Small"
    },
    "m": {
      "value": "$default * 1.5 ",
      "type": "sizing"
    },
    "l": {
      "value": "$default * 2",
      "type": "sizing",
      "description": "Large"
    },
    "xl": {
      "value": "$default * 3 ",
      "type": "sizing",
      "description": "Extra large"
    },
    "xxl": {
      "value": "$default * 4",
      "type": "sizing",
      "description": "Extra Extra large"
    },
    "xxxl": {
      "value": "$default * 8",
      "type": "sizing",
      "description": "XXX Large"
    },
    "theme": {
      "font": {
        "Header": {
          "value": "{global.colors.black}",
          "type": "color"
        },
        "subhead": {
          "value": "rgba({global.colors.black}, 35%)",
          "type": "color"
        }
      },
      "accent": {
        "dark": {
          "value": "{global.colors.primary.400}",
          "type": "color"
        },
        "light": {
          "value": "rgba({theme.accent.dark}, 10%)",
          "type": "color"
        }
      },
      "background": {
        "value": "{global.colors.white}",
        "type": "color"
      }
    },
    "font": {
      "Default": {
        "value": "Roboto",
        "type": "fontFamilies"
      }
    },
    "border": {
      "100": {
        "value": "100%",
        "type": "borderRadius"
      },
      "s": {
        "value": "2",
        "type": "borderRadius"
      },
      "m": {
        "value": "4",
        "type": "borderRadius"
      },
      "l": {
        "value": "16",
        "type": "borderRadius"
      },
      "xl": {
        "value": "24",
        "type": "borderRadius"
      }
    },
    "focus": {
      "value": [
        {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "4",
          "color": "{global.colors.yellow}",
          "type": "dropShadow"
        },
        {
          "x": "0",
          "y": "0",
          "blur": "0",
          "spread": "2",
          "color": "{global.colors.white}",
          "type": "dropShadow"
        }
      ],
      "type": "boxShadow"
    }
  },
  "font-change": {
    "font": {
      "Default": {
        "value": "Oswald",
        "type": "fontFamilies"
      }
    }
  },
  "dark": {
    "theme": {
      "font": {
        "Header": {
          "value": "{global.colors.white}",
          "type": "color"
        }
      },
      "background": {
        "value": "{global.colors.primary.900}",
        "type": "color"
      }
    }
  }
}